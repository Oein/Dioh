// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SourceCode {
  id String @id
  time DateTime @default(now())
  type String
  code String
  problem String
  usedTime String?
  usedMemory String?
}

model Problem {
  body String
  id String @id
  name String
  maxTime Int
  maxMemoryMB Int
  testCaseInputs String[]
  testCaseOutputs String[]
}